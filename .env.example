# Shopify Configuration
# Copy this file to .env and fill in your values

# Your Shopify store URL (e.g., your-store.myshopify.com)
SHOPIFY_STORE=

# Shopify API credentials (get these from your Shopify Partner Dashboard)
SHOPIFY_API_KEY=
SHOPIFY_API_SECRET=

# Shopify Admin API access token
SHOPIFY_ADMIN_API_ACCESS_TOKEN=

# Shopify Storefront API access token (for theme development)
SHOPIFY_STOREFRONT_API_ACCESS_TOKEN=

# Theme ID (optional, will be set automatically during development)
SHOPIFY_THEME_ID=


